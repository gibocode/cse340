<header id="top-header">
    <div class="siteName">
        <h1>
            <a href="/" title="Return to home page">
                <img src="/images/site/icon.webp" width="50" height="45" alt="CSE 340">
                <span class="montserrat-600">CSE Motors</span>
            </a>
        </h1>
    </div>
    <div id="tools">
        <% if (locals.loggedin) { %>
            <div>Welcome, <%= locals.accountData.account_firstname %>!</div>
            <a title="My Account" href="/account/">My Account</a>
            <a title="Management" href="/inv/">Management</a>
            <a title="Logout" href="/account/logout">Log Out</a>
        <% } else { %>
            <a title="Login" href="/account/login">Log In</a>
        <% } %>
    </div>
</header>
